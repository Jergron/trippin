{{#each this}}
<div id="showTrip#{{visited}}" class="dataSec" key="{{@key}}">
  <div>
    <h3>{{location}}</h3>
  </div>
  {{#each reviews}}
    <h4>{{title}} <small>{{date}}</small></h4>
  {{/each}}
  <div role="group" class=".btn-group btn-group-xs">
    <button role="group" class=".btn-group btn-group-xs btn btn-danger del">Remove Trip</button>
    <button class="btn btn-success" id="add-review#{{@key}}">Add Review</button> 
  </div>
</div>
{{/each}}